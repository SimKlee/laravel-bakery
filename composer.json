{
    "name":              "simklee/laravel-bakery",
    "description":       "A Laravel Bakery",
    "type":              "laravel-package",
    "license":           "MIT",
    "authors":           [
        {
            "name":  "SimKlee",
            "email": "simon.kleeschulte@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require":           {},
    "require-dev":       {
        "php":                         "^7.3|^8.0",
        "barryvdh/laravel-ide-helper": "^2.9",
        "phpunit/phpunit":             "^8.5 || ^9",
        "orchestra/testbench":         "^6"
    },
    "autoload":          {
        "psr-4": {
            "SimKlee\\LaravelBakery\\": "src"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "SimKlee\\LaravelBakery\\Tests\\": "tests"
        }
    },
    "extra":             {
        "laravel": {
            "providers": [
                "SimKlee\\LaravelBakery\\Providers\\LaravelBakeryServiceProvider"
            ]
        }
    }
}
